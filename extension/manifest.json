{
	"name": "PA User Agent for CrOS",
	"version": "2016.5.26.0",
	"manifest_version": #yourVersionNumber#,
	"app": {
		"background": {
			"scripts": ["jquery-2.1.3.min.js", "jquery.ajax-retry.min.js", "background.min.js"]
		}
	},
	"permissions": ["<all_urls>", "alarms", "identity", "identity.email", "system.network"],
	"storage": {"managed_schema": "schema.json"},
	"icons": { "16": "icon_16.png", "128": "icon_128.png" }
}
